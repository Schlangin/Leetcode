class Solution {                                   // THIS BRUTE FORCE METHOD WILL NOT PASS AS ITS INEFFICIENT
    public int[] productExceptSelf(int[] nums) {
        int[] answerArray = new int[nums.length];
        for (int i = 0; i < nums.length; i++) {
            int temp = 1;
            for (int j = 0; j < nums.length; j++) {
                if(i == j){
                    continue;
                }else{
                    temp = temp * nums[j];
                }
            }
            answerArray[i] = temp;
        }
        return answerArray;
    }
}

